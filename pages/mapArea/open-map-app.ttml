<view>
  <map id="myMap" enable-poi=true enable-building=true markers="{{markers}}" show-location scale="16" style="width: 100%; height: 100vh;"></map>
  <view class="map-btn-area">
    <view class="map-btn-text">{{destination}}</view>
    <button bindtap="openMapApp" type="primary">去导航</button>
  </view>
</view>
